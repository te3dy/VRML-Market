DEF	hop Transform {
			children [
				Shape {
					geometry Box {
						size 1 1 1
					}
					appearance Appearance {
									texture ImageTexture{
										  url "image/hop1.jpg"
										  repeatS TRUE
										  repeatT TRUE
										  }
								}
				}
		]
	}
	Transform {
		translation	0 1 0.75
		children USE hop
	}
	Transform {
		translation	0 0 1.5
		children USE hop
	}