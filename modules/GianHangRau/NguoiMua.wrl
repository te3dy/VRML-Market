############# cu dong tay

DEF	TS TimeSensor {
cycleInterval 4
loop TRUE 
startTime 0
stopTime 200}



DEF	IO OrientationInterpolator {
	key [0, .25, .5, .75]
	keyValue [-1 0 0 .2, -1 0 0 .4, 1 0 0 .2, 1 0 0 .4]

}


ROUTE ToS1.touchTime TO	TS.startTime


ROUTE TS.fraction_changed TO IO.set_fraction
ROUTE IO.value_changed TO cudongtay.rotation

ROUTE ToS2.touchTime TO	TS.stopTime



